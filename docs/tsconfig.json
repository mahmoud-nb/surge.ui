{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "bundler",
    "types": ["node", "vitepress/client"],
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": [".vitepress/**/*.ts", ".vitepress/**/*.vue"],
  "exclude": ["node_modules", ".vitepress/dist", ".vitepress/cache"]
}